void setup(){
 size(500,500);
 
}

void draw(){
background(255,255,255);
int sizeC=150;
for(int i=0; i<1; i++){
  fill(151, 76, 5);
  rect(210,270,100,200);
  fill(0,255,50);
ellipse(333-sizeC/2,230,sizeC,sizeC);
  fill(151, 76, 5);
  rect(320,280,100,200);
  fill(0,255,50);
ellipse(450-sizeC/2,230,sizeC,sizeC);
  fill(151, 76, 5);
  rect(100,260,100,200);
  fill(0,255,50);
ellipse(233-sizeC/2,230,sizeC,sizeC);

}
}
